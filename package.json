{
  "name": "re-reduced",
  "version": "2.2.1",
  "main": "dist/index.js",
  "umd:main": "dist/re-reduced.umd.production.js",
  "module": "dist/re-reduced.esm.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:coverage": "tsdx test --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "postversion": "git push && git push --tags",
    "docz": "docz dev",
    "docz:build": "docz build",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanrsoares/re-reduced.git"
  },
  "keywords": [
    "redux",
    "higher-order-reducer",
    "react",
    "immutable",
    "functional programming"
  ],
  "author": "Alan Soares <alanrsoars@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alanrsoares/re-reduced/issues"
  },
  "homepage": "https://github.com/alanrsoares/re-reduced#readme",
  "peerDependencies": {
    "react-redux": ">=7.1.0",
    "redux": ">=4",
    "redux-saga": ">=1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "pretty-quick",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/ramda": "^0.26.21",
    "@types/react": "^16.9.2",
    "@types/react-redux": "^7.1.2",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "docz": "^2.0.0-rc.31",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "~16.10.0",
    "react-dom": "~16.10.0",
    "react-hot-loader": "^4.12.12",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5",
    "redux-saga-test-plan": "^4.0.0-beta.4",
    "reselect": "^4.0.0",
    "styled-components": "^4.3.2",
    "tsdx": "^0.9.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "ramda": "^0.26.1"
  }
}

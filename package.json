{
  "name": "re-reduced",
  "version": "1.4.2",
  "description": "A utility toolbelt that reduces boilerplate from your react/redux app",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "test:coverage": "jest --config jestconfig.json --coverage",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "docz": "docz dev",
    "docz:build": "docz build",
    "typedoc": "typedoc --out api-reference src/lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanrsoares/re-reduced.git"
  },
  "keywords": [
    "redux",
    "higer-order-reducer",
    "react",
    "immutable",
    "functional programming"
  ],
  "author": "Alan Soares <alanrsoars@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alanrsoares/re-reduced/issues"
  },
  "homepage": "https://github.com/alanrsoares/re-reduced#readme",
  "peerDependencies": {
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "@types/jest": "^24.0.0",
    "@types/ramda": "^0.26.4",
    "@types/react-redux": "^7.0.5",
    "docz": "^1.0.0-rc.3",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^1.0.0-rc.3",
    "jest": "^24.0.0",
    "prettier": "^1.14.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1",
    "redux-saga-test-plan": "^4.0.0-beta.2",
    "reselect": "^4.0.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "ramda": "^0.26.1"
  }
}
